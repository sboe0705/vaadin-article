    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<managed-bean>
		<managed-bean-name>addressbookBean</managed-bean-name>
		<managed-bean-class>de.gebit.businessapplication.jsf.AddressbookBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
